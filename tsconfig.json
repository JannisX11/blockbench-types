{
	"include": ["./types/**/*"],
	"exclude": ["**/node_modules/**"],
	"compilerOptions": {
		"module": "commonjs",
		"target": "ES2020",
		"outDir": "out",
		"lib": ["dom", "ES2020"],
		"sourceMap": true,
		"rootDir": ".",
		"strict": true,
		"noImplicitAny": true,
		"esModuleInterop": true,
		"allowUmdGlobalAccess": true
	}
}
